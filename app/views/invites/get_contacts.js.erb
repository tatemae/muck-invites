<% if @success -%>
	jQuery("#<%=@render_contacts_into%>").html("<%=escape_javascript(contact_container(@contacts))%>");
	jQuery("#<%=@render_contacts_into%>").show();
	apply_ajax_forms();
<% else -%>
	<% if MuckEngine.configuration.growl_enabled -%>
	  jQuery.jGrowl.info(json.message);
	<% else -%>
	  <%= invite_ajax_message(@message) %>
	<% end -%>
	jQuery("#<%=@render_contacts_into%>").find('.waiting').hide();
	jQuery("#<%=@render_contacts_into%>").find('.invite-form').show();
	jQuery("#<%=@render_contacts_into%>").find('.wait-button').show();
<% end -%>
included_checkbox_by_email(checked_emails(), true);